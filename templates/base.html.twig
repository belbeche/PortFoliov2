<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="description" content="Nemesis studio">
        <meta name="author" content="walid belbeche">
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>WB</text></svg>">
        <link href="{{ asset('assets/css/bootstrap.css')}}" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

        <!-- Google fonts -->
        <link href="https://fonts.googleapis.com/css2?family=Mulish:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Poppins:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

        <link rel="stylesheet" href="{{ asset('assets/css/simplex.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/dark.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/plugins.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/style.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/css/app.css') }}">

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="dark iknow_tm_all_wrap" data-magic-cursor="show">
        <!-- PRELOADER -->
{#            <div id="preloader">#}
{#                <div class="loader_line"></div>#}
{#            </div>#}
        <!-- /PRELOADER -->
        <!-- MODALBOX -->
        <div class="iknow_tm_modalbox">
            <div class="box_inner">
                <div class="close">
                    <a href="#"><img class="svg" src="{{ asset('assets/img/svg/cancel.svg') }}" alt="" /></a>
                </div>
                <div class="description_wrap"></div>
            </div>
        </div>
        <!-- /MODALBOX -->
        <!-- TOPBAR -->
        <div class="iknow_tm_topbar">
            <div class="container">
                <div class="topbar_inner">
                    <div class="logo">
                        <span>En recherche d'alternance</span>
                    </div>
                    <div class="right">
                        <div class="iknow_tm_button fixedVersion">
                            <a href="{{ asset('assets/img/cv/1.jpg') }}" download>Télécharger CV</a>
                        </div>
                    </div>

                    <!-- AUTHOR INFO FOR MOBILE -->
                    <div class="iknow_tm_author_info">
                        <div class="info_inner">
                            <div class="info">
                                <h3>Walid BELBECHE</h3>
                                <span>Web developpeur</span>
                            </div>
                            <div class="avatar">
                                <div class="main" data-img-url="{{ asset('assets/img/hero/avatar.jpg') }}"></div>
                            </div>
                        </div>
                    </div>
                    <!-- /AUTHOR INFO FOR MOBILE -->

                </div>
            </div>
        </div>
        <!-- /TOPBAR -->
        <!-- BACKGROUND -->
        <div class="iknow_tm_fixed_background movingEffect" data-img-url="asset/img/hero/5.jpg"></div>
        <!-- /BACKGROUND -->
        <!-- CONTENT -->
        <div class="iknow_tm_fixed_content">
            <div class="container">
                <div class="fixed_content_inner">

                    <div class="content_left">
                        <!-- MENUBAR -->
                        <div class="menubar">
                            <ul>
                                <li class="active">
                                    <img class="svg" src="{{ asset('assets/img/svg/archive.svg') }}" alt="" />
                                    <span>A propos</span>
                                    <a class="iknow_tm_full_link" href="#about"></a>
                                </li>
                                <li>
                                    <img class="svg" src="{{ asset('assets/img/svg/contacs.svg') }}" alt="" />
                                    <span>Experiences</span>
                                    <a class="iknow_tm_full_link" href="#resume"></a>
                                </li>
                                <li>
                                    <img class="svg" src="{{ asset('assets/img/svg/briefcase.svg') }}" alt="" />
                                    <span>Portfolio</span>
                                    <a class="iknow_tm_full_link" href="#portfolio"></a>
                                </li>
                                <li>
                                    <img class="svg" src="{{ asset('assets/img/svg/gear.svg') }}" alt="" />
                                    <span>Services</span>
                                    <a class="iknow_tm_full_link" href="#service"></a>
                                </li>
                                <li>
                                    <img class="svg" src="{{ asset('assets/img/svg/message.svg') }}" alt="" />
                                    <span>Recommendation</span>
                                    <a class="iknow_tm_full_link" href="#testimonials"></a>
                                </li>
                                <li>
                                    <img class="svg" src="{{ asset('assets/img/svg/writing.svg') }}" alt="" />
                                    <span>Blog</span>
                                    <a class="iknow_tm_full_link" href="#news"></a>
                                </li>
                                <li>
                                    <img class="svg" src="{{ asset('assets/img/svg/letter.svg') }}" alt="" />
                                    <span>Contact</span>
                                    <a class="iknow_tm_full_link" href="#contact"></a>
                                </li>
                            </ul>
                        </div>
                        <!-- /MENUBAR -->
                    </div>
                    {% block body %}<div class="cover"></div>{% endblock %}
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        <script src="{{ asset('assets/js/jquery.js') }}"></script>
{#        <script src="{{ asset('assets/js/ie8.js') }}"></script>#}
        <script src="{{ asset('assets/js/init.js') }}"></script>
        <script src="{{ asset('assets/js/modernizr.custom.js') }}"></script>
        <script src="{{ asset('assets/js/plugins.js') }}"></script>
        <!-- MAGIC CURSOR -->
            <div class="mouse-cursor cursor-outer"></div>
            <div class="mouse-cursor cursor-inner"></div>
        <!-- /MAGIC CURSOR -->
    </body>
</html>
